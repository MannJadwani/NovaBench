  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Neo-Brutalist Landing</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <script>\n        tailwind.config = {\n            theme: {\n                extend: {\n                    fontFamily: {\n                        'brutal': ['Arial Black', 'sans-serif'],\n                    }\n                }\n            }\n        }\n    </script>\n</head>\n<body class=\"bg-yellow-300 font-brutal\">\n    <!-- Navigation -->\n    <nav class=\"bg-pink-400 border-b-4 border-black\">\n        <div class=\"container mx-auto px-4 py-3 flex justify-between items-center\">\n            <div class=\"text-3xl font-black\">BRUTAL</div>\n            <div class=\"flex space-x-6\">\n                <a href=\"#\" class=\"text-xl font-black border-b-2 border-black\">HOME</a>\n                <a href=\"#\" class=\"text-xl font-black\">ABOUT</a>\n                <a href=\"#\" class=\"text-xl font-black\">WORK</a>\n                <a href=\"#\" class=\"text-xl font-black\">CONTACT</a>\n            </div>\n        </div>\n    </nav>\n\n    <!-- Hero Section -->\n    <section class=\"bg-blue-400 py-16 border-b-4 border-black\">\n        <div class=\"container mx-auto px-4\">\n            <div class=\"max-w-4xl mx-auto text-center\">\n                <h1 class=\"text-6xl md:text-8xl font-black mb-6\">WE MAKE<br>RAW DIGITAL</h1>\n                <div class=\"bg-yellow-300 p-4 inline-block mb-8 border-4 border-black shadow-[8px_8px_0px_0px_rgba(0,0,0,1)]\">\n                    <p class=\"text-2xl md:text-4xl font-black\">NO-FUSS DESIGN FOR<br>THE DIGITAL AGE</p>\n                </div>\n                <button class=\"bg-pink-400 text-black px-10 py-4 text-2xl font-black border-4 border-black shadow-[8px_8px_0px_0px_rgba(0,0,0,1)] hover:shadow-[4px_4px_0px_0px_rgba(0,0,0,1)] transform hover:translate-x-1 hover:translate-y-1 transition-all\">\n                    GET STARTED\n                </button>\n            </div>\n        </div>\n    </section>\n\n    <!-- Features Section -->\n    <section class=\"bg-white py-16\">\n        <div class=\"container mx-auto px-4\">\n            <h2 class=\"text-5xl font-black mb-12 text-center\">WHAT WE DO</h2>\n            <div class=\"grid grid-cols-1 md:grid-cols-3 gap-8\">\n                <div class=\"bg-yellow-300 p-8 border-4 border-black shadow-[8px_8px_0px_0px_rgba(0,0,0,1)]\">\n                    <div class=\"bg-blue-400 w-24 h-24 border-4 border-black flex items-center justify-center mb-6\">\n                        <span class=\"text-5xl font-black\">A</span>\n                    </div>\n                    <h3 class=\"text-3xl font-black mb-4\">AUDACITY</h3>\n                    <p class=\"text-xl\">Bold designs that break conventions and stand out from the crowd.</p>\n                </div>\n                <div class=\"bg-pink-400 p-8 border-4 border-black shadow-[8px_8px_0px_0px_rgba(0,0,0,1)]\">\n                    <div class=\"bg-blue-400 w-24 h-24 border-4 border-black flex items-center justify-center mb-6\">\n                        <span class=\"text-5xl font-black\">B</span>\n                    </div>\n                    <h3 class=\"text-3xl font-black mb-4\">BRUTALITY</h3>\n                    <p class=\"text-xl\">Uncompromising approach to visual communication and aesthetics.</p>\n                </div>\n                <div class=\"bg-blue-400 p-8 border-4 border-black shadow-[8px_8px_0px_0px_rgba(0,0,0,1)]\">\n                    <div class=\"bg-yellow-300 w-24 h-24 border-4 border-black flex items-center justify-center mb-6\">\n                        <span class=\"text-5xl font-black\">C</span>\n                    </div>\n                    <h3 class=\"text-3xl font-black mb-4\">CLARITY</h3>\n                    <p class=\"text-xl\">Simple, direct messaging that cuts through the noise.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- CTA Section -->\n    <section class=\"bg-black py-16 border-t-4 border-b-4 border-white\">\n        <div class=\"container mx-auto px-4 text-center\">\n            <h2 class=\"text-5xl font-black text-white mb-6\">READY TO GO BRUTAL?</h2>\n            <p class=\"text-2xl text-yellow-300 mb-8 font-black\">Join the design revolution today</p>\n            <div class=\"flex justify-center space-x-4\">\n                <button class=\"bg-yellow-300 text-black px-8 py-4 text-xl font-black border-4 border-white shadow-[8px_8px_0px_0px_rgba(255,255,255,1)] hover:shadow-[4px_4px_0px_0px_rgba(255,255,255,1)] transform hover:translate-x-1 hover:translate-y-1 transition-all\">\n                    SIGN UP\n                </button>\n                <button class=\"bg-pink-400 text-white px-8 py-4 text-xl font-black border-4 border-white shadow-[8px_8px_0px_0px_rgba(255,255,255,1)] hover:shadow-[4px_4px_0px_0px_rgba(255,255,255,1)] transform hover:translate-x-1 hover:translate-y-1 transition-all\">\n                    LEARN MORE\n                </button>\n            </div>\n        </div>\n    </section>\n\n    <!-- Footer -->\n    <footer class=\"bg-blue-400 py-8 border-t-4 border-black\">\n        <div class=\"container mx-auto px-4\">\n            <div class=\"flex flex-col md:flex-row justify-between items-center\">\n                <div class=\"text-3xl font-black mb-4 md:mb-0\">BRUTAL</div>\n                <div class=\"flex space-x-4\">\n                    <div class=\"bg-black text-white w-12 h-12 flex items-center justify-center border-2 border-white\">\n                        <span class=\"text-xl font-black\">f</span>\n                    </div>\n                    <div class=\"bg-black text-white w-12 h-12 flex items-center justify-center border-2 border-white\">\n                        <span class=\"text-xl font-black\">t</span>\n                    </div>\n             