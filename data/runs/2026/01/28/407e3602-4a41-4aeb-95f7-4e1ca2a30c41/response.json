  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Jeans Web Design</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n</head>\n<body>\n    <div class=\"w-full h-screen bg-gray-100\">\n        <!-- Header Section -->\n        <header class=\"bg-white shadow-sm\">\n            <div class=\"container mx-auto px-6 py-4 flex justify-between items-center\">\n                <div class=\"font-bold text-xl\">JEANS</div>\n                <nav class=\"hidden md:flex space-x-8\">\n                    <a href=\"#\" class=\"hover:text-blue-600\">Home</a>\n                    <a href=\"#\" class=\"hover:text-blue-600\">About</a>\n                    <a href=\"#\" class=\"hover:text-blue-600\">Products</a>\n                    <a href=\"#\" class=\"hover:text-blue-600\">Contact</a>\n                </nav>\n            </div>\n        </header>\n\n        <!-- Hero Section -->\n        <section class=\"container mx-auto px-6 py-12\">\n            <h1 class=\"text-5xl font-bold text-gray-900 mb-4\">Quality Jeans<br>for Everyday Wear</h1>\n            <p class=\"text-lg text-gray-600 mb-8\">Experience comfort and style with our premium denim collection</p>\n            <button class=\"bg-black text-white px-6 py-3 rounded-md hover:bg-gray-700 transition duration-200\">Shop Now</button>\n        </section>\n\n        <!-- Featured Products -->\n        <section class=\"container mx-auto px-6 py-16\">\n            <h2 class=\"text-3xl font-bold text-gray-900 mb-12 text-center\">Featured Products</h2>\n            <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8\">\n                <div class=\"bg-white p-6 rounded-lg shadow-md\">\n                    <h3 class=\"text-xl font-semibold mb-2\">Classic Blue</h3>\n                    <p class=\"text-gray-600 text-sm mb-4\">$79.99</p>\n                    <img src=\"https://via.placeholder.com/400x300\" alt=\"Classic Blue Jeans\" class=\"w-full h-64 object-cover mb-6\">\n                    <button class=\"w-full bg-black text-white px-4 py-2 rounded hover:bg-gray-700 transition duration-200\">Add to Cart</button>\n                </div>\n                <div class=\"bg-white p-6 rounded-lg shadow-md\">\n                    <h3 class=\"text-xl font-semibold mb-2\">Slim Fit Black</h3>\n                    <p class=\"text-gray-600 text-sm mb-4\">$89.99</p>\n                    <img src=\"https://via.placeholder.com/400x300\" alt=\"Slim Fit Black Jeans\" class=\"w-full h-64 object-cover mb-6\">\n                    <button class=\"w-full bg-black text-white px-4 py-2 rounded hover:bg-gray-700 transition duration-200\">Add to Cart</button>\n                </div>\n                <div class=\"bg-white p-6 rounded-lg shadow-md\">\n                    <h3 class=\"text-xl font-semibold mb-2\">Ripped Indigo</h3>\n                    <p class=\"text-gray-600 text-sm mb-4\">$84.99</p>\n                    <img src=\"https://via.placeholder.com/400x300\" alt=\"Ripped Indigo Jeans\" class=\"w-full h-64 object-cover mb-6\">\n                    <button class=\"w-full bg-black text-white px-4 py-2 rounded hover:bg-gray-700 transition duration-200\">Add to Cart</button>\n                </div>\n            </div>\n        </section>\n    </div>\n</body>\n</html>"
}