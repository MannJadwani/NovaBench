  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Swiss Design Agency</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css\">\n    <style>\n        @import url('https://fonts.googleapis.com/css2?family=Helvetica:wght@300;400;700&display=swap');\n        body {\n            font-family: 'Helvetica', 'Arial', sans-serif;\n        }\n    </style>\n</head>\n<body class=\"bg-white text-black\">\n    <!-- Grid Container -->\n    <div class=\"container mx-auto px-4 max-w-7xl\">\n        \n        <!-- Navigation -->\n        <nav class=\"py-8 grid grid-cols-12 items-center\">\n            <div class=\"col-span-4\">\n                <div class=\"text-2xl font-bold tracking-wide\">DESIGN</div>\n            </div>\n            <div class=\"col-span-8 text-right\">\n                <ul class=\"inline-flex space-x-8 text-sm font-medium tracking-wide\">\n                    <li><a href=\"#work\" class=\"hover:text-red-600 transition-colors\">WORK</a></li>\n                    <li><a href=\"#services\" class=\"hover:text-red-600 transition-colors\">SERVICES</a></li>\n                    <li><a href=\"#about\" class=\"hover:text-red-600 transition-colors\">ABOUT</a></li>\n                    <li><a href=\"#contact\" class=\"hover:text-red-600 transition-colors\">CONTACT</a></li>\n                </ul>\n            </div>\n        </nav>\n        \n        <!-- Hero Section -->\n        <section class=\"py-24 grid grid-cols-12\">\n            <div class=\"col-span-8\">\n                <h1 class=\"text-6xl font-bold leading-tight mb-8\">DESIGN WITH PURPOSE AND CLARITY</h1>\n                <p class=\"text-lg mb-12 max-w-2xl\">Swiss-inspired design studio creating functional, beautiful solutions for the modern world.</p>\n                <div class=\"flex space-x-4\">\n                    <button class=\"bg-black text-white px-8 py-3 text-sm font-bold tracking-wider hover:bg-gray-800 transition-colors\">VIEW WORK</button>\n                    <button class=\"border-2 border-black px-8 py-3 text-sm font-bold tracking-wider hover:bg-black hover:text-white transition-colors\">LEARN MORE</button>\n                </div>\n            </div>\n            <div class=\"col-span-4\">\n                <div class=\"bg-red-600 h-full w-1/2 ml-auto\"></div>\n            </div>\n        </section>\n        \n        <!-- Work Section -->\n        <section id=\"work\" class=\"py-20 border-t-2 border-black\">\n            <div class=\"mb-16\">\n                <h2 class=\"text-3xl font-bold mb-2\">SELECTED WORK</h2>\n                <div class=\"h-1 w-24 bg-red-600 mb-8\"></div>\n            </div>\n            \n            <div class=\"grid grid-cols-12 gap-8\">\n                <!-- Project 1 -->\n                <div class=\"col-span-6 group cursor-pointer\">\n                    <div class=\"overflow-hidden mb-4\">\n                        <div class=\"bg-gray-200 h-80 transform transition-transform group-hover:scale-105\"></div>\n                    </div>\n                    <h3 class=\"text-xl font-bold mb-1\">BRAND IDENTITY</h3>\n                    <p class=\"text-sm text-gray-600\">2023</p>\n                </div>\n                \n                <!-- Project 2 -->\n                <div class=\"col-span-6 group cursor-pointer\">\n                    <div class=\"overflow-hidden mb-4\">\n                        <div class=\"bg-gray-200 h-80 transform transition-transform group-hover:scale-105\"></div>\n                    </div>\n                    <h3 class=\"text-xl font-bold mb-1\">WEB DESIGN</h3>\n                    <p class=\"text-sm text-gray-600\">2023</p>\n                </div>\n                \n                <!-- Project 3 -->\n                <div class=\"col-span-4 group cursor-pointer\">\n                    <div class=\"overflow-hidden mb-4\">\n                        <div class=\"bg-gray-200 h-64 transform transition-transform group-hover:scale-105\"></div>\n                    </div>\n                    <h3 class=\"text-xl font-bold mb-1\">PRINT DESIGN</h3>\n                    <p class=\"text-sm text-gray-600\">2022</p>\n                </div>\n                \n                <!-- Project 4 -->\n                <div class=\"col-span-4 group cursor-pointer\">\n                    <div class=\"overflow-hidden mb-4\">\n                        <div class=\"bg-gray-200 h-64 transform transition-transform group-hover:scale-105\"></div>\n                    </div>\n                    <h3 class=\"text-xl font-bold mb-1\">PACKAGING</h3>\n                    <p class=\"text-sm text-gray-600\">2022</p>\n                </div>\n                \n                <!-- Project 5 -->\n                <div class=\"col-span-4 group cursor-pointer\">\n                    <div class=\"overflow-hidden mb-4\">\n                        <div class=\"bg-gray-200 h-64 transform transition-transform group-hover:scale-105\"></div>\n                    </div>\n                    <h3 class=\"text-xl font-bold mb-1\">EDITORIAL</h3>\n                    <p class=\"text-sm text-gray-600\">2021</p>\n                </div>\n            </div>\n        </section>\n        \n        <!-- Services Section -->\n        <section id=\"services\" class=\"py-20 border-t-2 border-black\">\n            <div class=\"grid grid-cols-12\">\n                <div class=\"col-span-4 mb-12\">\n                    <h2 class=\"text-3xl font-bold mb-2\">OUR SERVICES</h2>\n                    <div class=\"h-1 w-24 bg-red-600 mb-6\"></div>\n                    <p class=\"text-gray-700\">We provide comprehensive design solutions rooted in Swiss design principles.</p>\n                </div>\n                \n                <div class=\"col-span-8 grid grid-cols-2 gap-8\">\n                    <div>\n                        <div class=\"mb-6\">\n                            <h3 class=\"text-xl font-bold mb-2\">BRAND IDENTITY</h3>\n                            <div class=\"h-1 w-12 bg-black mb-4\"></div>\n                            <p class=\"text-gray-700\">Logo design, brand guidelines, visual identity systems.</p>\n                        </div>\n                        \n                        <div class=\"mb-6\">\n                            <h3 class=\"text-xl font-bold mb-2\">WEB DESIGN</h3>\n                            <div class=\"h-1 w-12 bg-black mb-4\"></div>\n                            <p class=\"text-gray-700\">Responsive websites, user interfaces, digital experiences.</p>\n                        </div>\n                    </div>\n                    \n                    <div>\n                        <div class=\"mb-6\">\n                            <h3 class=\"text-xl font-bold mb-2\">PRINT DESIGN</h3>\n                            <div class=\"h-1 w-12 bg-black mb-4\"></div>\n                            <p class=\"text-gray-700\">Brochures, posters, publications, packaging.</p>\n                        </div>\n                        \n                        <div class=\"mb-6\">\n                            <h3 class=\"text-xl font-bold mb-2\">CONSULTING</h3>\n                            <div class=\"h-1 w-12 bg-black mb-4\"></div>\n                            <p class=\"text-gray-700\">Design strategy, art direction, workshops.</p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n        \n        <!-- About Section -->\n        <section id=\"about\" class=\"py-20 border-t-2 border-black\">\n            <div class=\"grid grid-cols-12\">\n                <div class=\"col-span-12 mb-16\">\n                    <h2 class=\"text-3xl font-bold mb-2\">ABOUT US</h2>\n                    <div class=\"h-1 w-24 bg-red-600\"></div>\n                </div>\n                \n                <div class=\"col-span-6 mb-16\">\n                    <h3 class=\"text-xl font-bold mb-4\">OUR PHILOSOPHY</h3>\n                    <p class=\"text-gray-700 mb-4\">We believe in the power of simplicity and clarity. Our approach is guided by the principles of Swiss Design: functionality, minimalism, and attention to detail.</p>\n                    <p class=\"text-gray-700\">Every project we undertake is an opportunity to create something meaningful and timeless, where form follows function and aesthetics serve purpose.</p>\n                </div>\n                \n                <div class=\"col-span-6 mb-16\">\n                    <h3 class=\"text-xl font-bold mb-4\">OUR PROCESS</h3>\n                    <ul class=\"space-y-4\">\n                        <li class=\"flex\">\n                            <div class=\"mr-4\">\n                                <div class=\"w-8 h-8 rounded-full bg-black flex items-center justify-center\">\n                                    <span class=\"text-white text-sm font-bold\">1</span>\n                                </div>\n                            </div>\n                            <div>\n                                <h4 class=\"font-bold mb-1\">RESEARCH</h4>\n                                <p class=\"text-sm text-gray-700\">Understanding your needs and goals</p>\n                            </div>\n                        </li>\n                        \n                        <li class=\"flex\">\n                            <div class=\"mr-4\">\n                                <div class=\"w-8 h-8 rounded-full bg-black flex items-center justify-center\">\n                                    <span class=\"text-white text-sm font-bold\">2</span>\n                                </div>\n                            </div>\n                            <div>\n                                <h4 class=\"font-bold mb-1\">CONCEPT</h4>\n                                <p class=\"text-sm text-gray-700\">Developing strategic design solutions</p>\n                            </div>\n                        </li>\n                        \n                        <li class=\"flex\">\n                            <div class=\"mr-4\">\n                                <div class=\"w-8 h-8 rounded-full bg-black flex items-center justify-center\">\n                                    <span class=\"text-white text-sm font-bold\">3</span>\n                                </div>\n                            </div>\n                            <div>\n                                <h4 class=\"font-bold mb-1\">EXECUTION</h4>\n                                <p class=\"text-sm text-gray-700\">Precision implementation and delivery</p>\n                            </div>\n                        </li>\n                    </ul>\n                </div>\n                \n                <div class=\"col-span-12 grid grid-cols-4 gap-4\">\n                    <div class=\"col-span-1\">\n                        <div class=\"text-5xl font-bold\">15+</div>\n                        <div class=\"text-sm text-gray-700 mt-2\">YEARS EXPERIENCE</div>\n                    </div>\n                    <div class=\"col-span-1\">\n                        <div class=\"text-5xl font-bold\">120+</div>\n                        <div class=\"text-sm text-gray-700 mt-2\">PROJECTS COMPLETED</div>\n                    </div>\n                    <div class=\"col-span-1\">\n                        <div class=\"text-5xl font-bold\">85+</div>\n                        <div class=\"text-sm text-gray-700 mt-2\">HAPPY CLIENTS</div>\n                    </div>\n                    <div class=\"col-span-1\">\n                        <div class=\"text-5xl font-bold\">12</div>\n                        <div class=\"text-sm text-gray-700 mt-2\">DESIGN AWARDS</div>\n                    </div>\n                </div>\n            </div>\n        </section>\n        \n        <!-- Contact Section -->\n        <section id=\"contact\" class=\"py-20 border-t-2 border-black\">\n            <div class=\"grid grid-cols-12\">\n                <div class=\"col-span-12 mb-16\">\n                    <h2 class=\"text-3xl font-bold mb-2\">CONTACT</h2>\n                    <div class=\"h-1 w-24 bg-red-600\"></div>\n                </div>\n                \n                <div class=\"col-span-6\">\n                    <h3 class=\"text-xl font-bold mb-8\">LET'S WORK TOGETHER</h3>\n                    \n                    <div class=\"mb-8\">\n                        <h4 class=\"font-bold mb-4\">EMAIL</h4>\n                        <p class=\"text-gray-700\">hello@swissdesign.com</p>\n                    </div>\n                    \n                    <div class=\"mb-8\">\n                        <h4 class=\"font-bold mb-4\">STUDIO</h4>\n                        <p class=\"text-gray-700 mb-1\">123 Design Street</p>\n                        <p class=\"text-gray-700 mb-1\">Zurich, Switzerland</p>\n                        <p class=\"text-gray-700\">+41 44 123 45 67</p>\n                    </div>\n                    \n                    <div class=\"mb-8\">\n                        <h4 class=\"font-bold mb-4\">FOLLOW</h4>\n                        <div class=\"flex space-x-4\">\n                            <a href=\"#\" class=\"hover:text-red-600 transition-colors\">\n                                <i class=\"fab fa-instagram text-xl\"></i>\n                            </a>\n                            <a href=\"#\" class=\"hover:text-red-600 transition-colors\">\n                                <i class=\"fab fa-behance text-xl\"></i>\n                            </a>\n                            <a href=\"#\" class=\"hover:text-red-600 transition-colors\">\n                                <i class=\"fab fa-linkedin text-xl\"></i>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n                \n                <div class=\"col-span-6\">\n                    <form class=\"space-y-6\">\n                        <div>\n                            <label class=\"block text-sm font-bold mb-2\">NAME</label>\n                            <input type=\"text\" class=\"w-full border-b-2 border-black focus:border-red-600 outline-none py-2\">\n                        </div>\n                        \n                        <div>\n                            <label class=\"block text-sm font-bold mb-2\">EMAIL</label>\n                            <input type=\"email\" class=\"w-full border-b-2 border-black focus:border-red-600 outline-none py-2\">\n                        </div>\n                        \n                        <div>\n                            <label class=\"block text-sm font-bold mb-2\">MESSAGE</label>\n                            <textarea class=\"w-full border-b-2 border-black focus:border-red-600 outline-none py-2 h-32\"></textarea>\n                        </div>\n                        \n                        <button type=\"submit\" class=\"bg-black text-white px-8 py-3 text-sm font-bold tracking-wider hover:bg-gray-800 transition-colors\">SEND MESSAGE</button>\n                    </form>\n                </div>\n            </div>\n        </section>\n        \n        <!-- Footer -->\n        <footer class=\"py-16 border-t-2 border-black\">\n            <div class=\"grid grid-cols-12\">\n                <div class=\"col-span-6\">\n                    <div class=\"text-xl font-bold mb-4\">DESIGN</div>\n                    <p class=\"text-sm text-gray-700\">Â© 2023 Swiss Design Studio. All rights reserved.</p>\n                </div>\n                \n                <div class=\"col-span-6 text-right text-sm\">\n                    <a href=\"#\" class=\"block mb-2 hover:text-red-600 transition-colors\">PRIVACY POLICY</a>\n                    <a href=\"#\" class=\"block hover:text-red-600 transition-colors\">TERMS OF SERVICE</a>\n                </div>\n            </div>\n        </footer>\n    </div>\n</body>\n</html>"
}