  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Swiss Design Landing Page</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n</head>\n<body class=\"bg-white text-slate-900 font-sans\">\n    <!-- Header -->\n    <header class=\"container mx-auto px-4 max-w-6xl\">\n        <div class=\"md:flex md:items-center md:justify-between py-8\">\n            <div class=\"md:w-1/3\">\n                <h1 class=\"text-6xl md:text-8xl font-bold leading-none tracking-tight\">Swiss</h1>\n            </div>\n            \n            <nav class=\"hidden md:flex md:w-2/3 justify-end gap-12 text-sm md:text-base\">\n                <a href=\"#work\" class=\"hover:text-slate-600 transition-colors\">Work</a>\n                <a href=\"#about\" class=\"hover:text-slate-600 transition-colors\">About</a>\n                <a href=\"#process\" class=\"hover:text-slate-600 transition-colors\">Process</a>\n                <a href=\"#contact\" class=\"hover:text-slate-600 transition-colors\">Contact</a>\n            </nav>\n        </div>\n    </header>\n\n    <!-- Hero -->\n    <section class=\"container mx-auto px-4 max-w-6xl mb-24\">\n        <div class=\"md:grid md:grid-cols-2 md:gap-16 items-center\">\n            <div class=\"md:w-1/2 mb-12 md:mb-0\">\n                <div class=\"p-8 bg-slate-200 text-left\">\n                    <h2 class=\"text-3xl md:text-6xl font-bold mb-4 leading-tight tracking-tight\">Functional. Minimalist. Timeless.</h2>\n                    <p class=\"text-xl md:text-2xl text-slate-600 mb-8\">We create clear, purposeful design that works beautifully.</p>\n                    <p class=\"text-sm md:text-base text-slate-500\">Culture · Innovation · Precision</p>\n                </div>\n            </div>\n            <div class=\"md:w-1/2\">\n                <div class=\"p-6 bg-slate-200\">\n                    <div class=\"p-8 bg-black text-white\">\n                        <p class=\"text-sm\">project lorem ipsum</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Work -->\n    <section id=\"work\" class=\"container mx-auto px-4 max-w-6xl mb-24\">\n        <div class=\"text-center mb-16\">\n            <h3 class=\"text-4xl md:text-6xl font-bold tracking-tight mb-6\">Work</h3>\n            <p class=\"text-lg text-slate-600\">Recent projects and case studies</p>\n        </div>\n\n        <div class=\"md:grid md:grid-cols-3 md:gap-8\">\n            <div class=\"md:col-span-2\">\n                <div class=\"grid grid-cols-3 gap-6 mb-6\">\n                    <div class=\"col-span-2 bg-slate-200 h-64\"></div>\n                    <div class=\"bg-black h-64\"></div>\n                </div>\n                <div class=\"grid grid-cols-4 gap-6\">\n                    <div class=\"bg-slate-200 h-48\"></div>\n                    <div class=\"bg-slate-200 h-48\"></div>\n                    <div class=\"bg-slate-200 h-48\"></div>\n                    <div class=\"bg-black h-48\"></div>\n                </div>\n            </div>\n            <div class=\"md:col-span-1 space-y-6\">\n                <div class=\"p-8 bg-slate-200\">\n                    <p class=\"uppercase tracking-widest text-xs text-slate-500 mb-3\">Case Study</p>\n                    <h4 class=\"text-2xl font-bold\">Brand Identity</h4>\n                    <p class=\"text-slate-600 mt-4\">A comprehensive visual language that communicates your brand's essence with clarity and precision.</p>\n                </div>\n                <div class=\"p-8 bg-slate-200\">\n                    <p class=\"uppercase tracking-widest text-xs text-slate-500 mb-3\">Case Study</p>\n                    <h4 class=\"text-2xl font-bold\">Web Design</h4>\n                    <p class=\"text-slate-600 mt-4\">Clean, functional interfaces that prioritize user experience and thoughtful information hierarchy.</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <!-- Stats -->\n    <section class=\"container mx-auto px-4 max-w-6xl mb-24\">\n        <div class=\"grid grid-cols-4 gap-8 text-center\">\n            <div>\n                <h4 class=\"text-4xl font-bold\">48</h4>\n                <p class=\"text-sm uppercase tracking-widest text-slate-600\">Projects</p>\n            </div>\n            <div>\n                <h4 class=\"text-4xl font-bold\">24/7</h4>\n                <p class=\"text-sm uppercase tracking-widest text-slate-600\">Support</p>\n            </div>\n            <div>\n                <h4 class=\"text-4xl font-bold\">97%</h4>\n                <p class=\"text-sm uppercase tracking-widest text-slate-600\">Success</p>\n            </div>\n            <div>\n                <h4 class=\"text-4xl font-bold\">37</h4>\n                <p class=\"text-sm uppercase tracking-widest text-slate-600\">Awards</p>\n            </div>\n        </div>\n    </section>\n\n    <!-- Footer -->\n    <footer class=\"container mx-auto px-4 max-w-6xl py-16\">\n        <div class=\"md:flex md:items-center md:justify-between\">\n            <div class=\"md:w-1/3 mb-8 md:mb-0\">\n                <h1 class=\"text-5xl font-bold tracking-tight\">Swiss</h1>\n            </div>\n            <nav class=\"md:w-2/3 text-right\">\n                <p class=\"text-slate-600 md:text-left\">&copy; 2024 Swiss Design Studio. Made with precision in Switzerland.</p>\n            </nav>\n        </div>\n    </footer>\n</body>\n</html>"
}