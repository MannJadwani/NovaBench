  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Dashboard</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css\">\n</head>\n<body class=\"bg-gray-50 font-sans\">\n    <!-- Navigation Bar -->\n    <nav class=\"bg-white shadow-md\">\n        <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n            <div class=\"flex justify-between h-16\">\n                <div class=\"flex items-center\">\n                    <div class=\"flex-shrink-0 flex items-center\">\n                        <div class=\"bg-indigo-600 h-8 w-8 rounded-md flex items-center justify-center\">\n                            <span class=\"text-white font-bold\">B</span>\n                        </div>\n                        <span class=\"ml-2 text-xl font-bold text-gray-800\">Brand</span>\n                    </div>\n                    <div class=\"hidden md:ml-10 md:flex md:space-x-8\">\n                        <a href=\"#\" class=\"text-indigo-600 border-indigo-500 border-b-2 px-1 pt-1 text-sm font-medium\">Dashboard</a>\n                        <a href=\"#\" class=\"text-gray-500 hover:text-gray-700 border-transparent hover:border-gray-300 border-b-2 px-1 pt-1 text-sm font-medium\">Projects</a>\n                        <a href=\"#\" class=\"text-gray-500 hover:text-gray-700 border-transparent hover:border-gray-300 border-b-2 px-1 pt-1 text-sm font-medium\">Calendar</a>\n                        <a href=\"#\" class=\"text-gray-500 hover:text-gray-700 border-transparent hover:border-gray-300 border-b-2 px-1 pt-1 text-sm font-medium\">Documents</a>\n                    </div>\n                </div>\n                <div class=\"flex items-center\">\n                    <button class=\"p-1 rounded-full text-gray-400 hover:text-gray-500 focus:outline-none\">\n                        <i class=\"fas fa-search text-gray-500\"></i>\n                    </button>\n                    <button class=\"ml-3 p-1 rounded-full text-gray-400 hover:text-gray-500 focus:outline-none\">\n                        <i class=\"fas fa-bell text-gray-500\"></i>\n                    </button>\n                    <div class=\"ml-3 relative flex-shrink-0\">\n                        <div class=\"flex items-center\">\n                            <img class=\"h-8 w-8 rounded-full\" src=\"https://randomuser.me/api/portraits/men/1.jpg\" alt=\"User\">\n                            <span class=\"ml-2 text-sm font-medium text-gray-700\">John Doe</span>\n                            <i class=\"fas fa-chevron-down ml-1 text-xs text-gray-500\"></i>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </nav>\n\n    <!-- Main Content -->\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8\">\n        <div class=\"flex flex-col md:flex-row gap-8\">\n            <!-- Left Sidebar -->\n            <div class=\"w-full md:w-1/4\">\n                <div class=\"bg-white rounded-lg shadow-md p-6\">\n                    <div class=\"flex flex-col items-center\">\n                        <img class=\"h-24 w-24 rounded-full object-cover mb-4\" src=\"https://randomuser.me/api/portraits/men/1.jpg\" alt=\"User\">\n                        <h2 class=\"text-xl font-bold text-gray-800\">John Doe</h2>\n                        <p class=\"text-gray-600 text-sm\">Admin</p>\n                        <div class=\"mt-4 w-full\">\n                            <div class=\"flex justify-between text-sm text-gray-600 mb-1\">\n                                <span>Profile Completion</span>\n                                <span>75%</span>\n                            </div>\n                            <div class=\"w-full bg-gray-200 rounded-full h-2\">\n                                <div class=\"bg-indigo-600 h-2 rounded-full\" style=\"width: 75%\"></div>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <div class=\"mt-6\">\n                        <h3 class=\"text-sm font-medium text-gray-500 uppercase tracking-wider mb-3\">Recent Activity</h3>\n                        <ul class=\"space-y-2\">\n                            <li class=\"flex items-start\">\n                                <div class=\"flex-shrink-0\">\n                                    <div class=\"h-2 w-2 bg-indigo-600 rounded-full mt-2\"></div>\n                                </div>\n                                <div class=\"ml-3 text-sm text-gray-600\">\n                                    <p>Updated dashboard</p>\n                                    <p class=\"text-xs text-gray-500\">2 hours ago</p>\n                                </div>\n                            </li>\n                            <li class=\"flex items-start\">\n                                <div class=\"flex-shrink-0\">\n                                    <div class=\"h-2 w-2 bg-indigo-600 rounded-full mt-2\"></div>\n                                </div>\n                                <div class=\"ml-3 text-sm text-gray-600\">\n                                    <p>Added new project</p>\n                                    <p class=\"text-xs text-gray-500\">1 day ago</p>\n                                </div>\n                            </li>\n                            <li class=\"flex items-start\">\n                                <div class=\"flex-shrink-0\">\n                                    <div class=\"h-2 w-2 bg-indigo-600 rounded-full mt-2\"></div>\n                                </div>\n                                <div class=\"ml-3 text-sm text-gray-600\">\n                                    <p>Completed task</p>\n                                    <p class=\"text-xs text-gray-500\">3 days ago</p>\n                                </div>\n                            </li>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n\n            <!-- Main Content Area -->\n            <div class=\"w-full md:w-3/4\">\n                <!-- Welcome Banner -->\n                <div class=\"bg-gradient-to-r from-indigo-600 to-purple-600 rounded-lg shadow-md p-6 mb-8\">\n                    <h1 class=\"text-2xl font-bold text-white\">Welcome back, John!</h1>\n                    <p class=\"text-indigo-100 mt-1\">Here's what's happening with your projects today.</p>\n                    \n                    <div class=\"mt-6 grid grid-cols-1 md:grid-cols-3 gap-4\">\n                        <div class=\"bg-white bg-opacity-20 rounded-lg p-4\">\n                            <p class=\"text-indigo-100 text-sm\">Total Projects</p>\n                            <p class=\"text-2xl font-bold text-white\">12</p>\n                        </div>\n                        <div class=\"bg-white bg-opacity-20 rounded-lg p-4\">\n                            <p class=\"text-indigo-100 text-sm\">Completed Tasks</p>\n                            <p class=\"text-2xl font-bold text-white\">84</p>\n                        </div>\n                        <div class=\"bg-white bg-opacity-20 rounded-lg p-4\">\n                            <p class=\"text-indigo-100 text-sm\">Pending Tasks</p>\n                            <p class=\"text-2xl font-bold text-white\">16</p>\n                        </div>\n                    </div>\n                </div>\n\n                <!-- Chart Section -->\n                <div class=\"bg-white rounded-lg shadow-md p-6\">\n                    <div class=\"flex justify-between items-center mb-6\">\n                        <h2 class=\"text-lg font-bold text-gray-800\">Performance Overview</h2>\n                        <div class=\"flex space-x-2\">\n                            <button class=\"px-3 py-1 text-sm bg-indigo-600 text-white rounded-md\">Week</button>\n                            <button class=\"px-3 py-1 text-sm bg-gray-200 text-gray-700 rounded-md hover:bg-gray-300\">Month</button>\n                            <button class=\"px-3 py-1 text-sm bg-gray-200 text-gray-700 rounded-md hover:bg-gray-300\">Year</button>\n                        </div>\n                    </div>\n                    \n                    <!-- Chart Placeholder -->\n                    <div class=\"h-64 bg-gray-100 rounded-lg flex items-center justify-center\">\n                        <div class=\"text-center\">\n                            <i class=\"fas fa-chart-line text-4xl text-gray-400 mb-2\"></i>\n                            <p class=\"text-gray-500\">Performance Chart</p>\n                        </div>\n                    </div>\n                    \n                    <!-- Chart Legend -->\n                    <div class=\"mt-6 flex flex-wrap gap-4\">\n                        <div class=\"flex items-center\">\n                            <div class=\"h-3 w-3 bg-indigo-600 rounded-full\"></div>\n                            <span class=\"ml-2 text-sm text-gray-600\">Tasks Completed</span>\n                        </div>\n                        <div class=\"flex items-center\">\n                            <div class=\"h-3 w-3 bg-purple-600 rounded-full\"></div>\n                            <span class=\"ml-2 text-sm text-gray-600\">Tasks Pending</span>\n                        </div>\n                        <div class=\"flex items-center\">\n                            <div class=\"h-3 w-3 bg-green-500 rounded-full\"></div>\n                            <span class=\"ml-2 text-sm text-gray-600\">Efficiency</span>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</body>\n</html>"
}