  "text": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Cyberpunk Landing</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <style>\n        @import url('https://fonts.googleapis.com/css2?family=Acme&family=Orbitron:wght@400;700&display=swap');\n    </style>\n</head>\n<body class=\"bg-gray-900 text-white\">\n    <div class=\"container mx-auto px-4 h-screen flex flex-col justify-center items-center\">\n        <div class=\"text-center\">\n            <h1 class=\"text-6xl font-bold font-acme text-gradient-to-r from-pink-400 to-purple-400 text-transparent bg-clip-text opacity-90 mb-4\">\n                NEON WASTELAND\n            </h1>\n            <p class=\"text-xl font-orbitron mb-8 opacity-70\">\n                Welcome to the future. It's all glitch and neon.\n            </p>\n            <div class=\"space-x-4\">\n                <button class=\"px-8 py-3 bg-pink-500 hover:bg-pink-400 text-white font-bold rounded-full transition-all\">\n                    Sign Up\n                </button>\n                <button class=\"px-8 py-3 bg-purple-500 hover:bg-purple-400 text-white font-bold rounded-full transition-all\">\n                    Learn More\n                </button>\n            </div>\n        </div>\n    </div>\n\n    <footer class=\"fixed bottom-4 text-sm text-gray-400\">\n        <p class=\"animate-pulse\">\n            <span class=\"text-pink-400\">>>></span> SYNTAX ERROR. FUTURE OVERLOAD DETECTED.\n        </p>\n    </footer>\n</body>\n</html>"
}